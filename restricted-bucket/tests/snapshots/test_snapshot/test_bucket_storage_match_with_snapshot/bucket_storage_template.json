{
    "Parameters": {
        "BootstrapVersion": {
            "Default": "/cdk-bootstrap/hnb659fds/version",
            "Description": "Version of the CDK Bootstrap resources in this environment, automatically retrieved from SSM Parameter Store. [cdk:skip]",
            "Type": "AWS::SSM::Parameter::Value<String>"
        }
    },
    "Resources": {
        "RestrictedFilesBucket40395BF1": {
            "DeletionPolicy": "Retain",
            "Properties": {
                "BucketName": "igvf-restricted-files",
                "CorsConfiguration": {
                    "CorsRules": [
                        {
                            "AllowedHeaders": [
                                "*"
                            ],
                            "AllowedMethods": [
                                "GET",
                                "HEAD",
                                "POST",
                                "PUT"
                            ],
                            "AllowedOrigins": [
                                "https://*-script.googleusercontent.com"
                            ],
                            "ExposedHeaders": [
                                "Content-Length",
                                "Content-Range",
                                "Content-Type",
                                "ETag"
                            ],
                            "MaxAge": 3000
                        },
                        {
                            "AllowedHeaders": [
                                "Accept",
                                "Origin",
                                "Range",
                                "X-Requested-With",
                                "Cache-Control"
                            ],
                            "AllowedMethods": [
                                "GET",
                                "HEAD"
                            ],
                            "AllowedOrigins": [
                                "*"
                            ],
                            "ExposedHeaders": [
                                "Content-Length",
                                "Content-Range",
                                "Content-Type"
                            ],
                            "MaxAge": 3000
                        }
                    ]
                },
                "LifecycleConfiguration": {
                    "Rules": [
                        {
                            "Id": "move-all-objects-to-intelligent-tiering",
                            "Status": "Enabled",
                            "Transitions": [
                                {
                                    "StorageClass": "INTELLIGENT_TIERING",
                                    "TransitionInDays": 0
                                }
                            ]
                        },
                        {
                            "AbortIncompleteMultipartUpload": {
                                "DaysAfterInitiation": 7
                            },
                            "Id": "delete-incomplete-multipart-uploads",
                            "Status": "Enabled"
                        },
                        {
                            "Id": "send-old-versions-to-glacier",
                            "NoncurrentVersionExpiration": {
                                "NoncurrentDays": 30
                            },
                            "NoncurrentVersionTransitions": [
                                {
                                    "StorageClass": "GLACIER",
                                    "TransitionInDays": 0
                                }
                            ],
                            "Status": "Enabled"
                        },
                        {
                            "Id": "send-tagged-objects-to-glacier",
                            "Status": "Enabled",
                            "TagFilters": [
                                {
                                    "Key": "send_to_glacier",
                                    "Value": "true"
                                }
                            ],
                            "Transitions": [
                                {
                                    "StorageClass": "DEEP_ARCHIVE",
                                    "TransitionInDays": 0
                                }
                            ]
                        },
                        {
                            "Id": "send-objects-copied-to-open-data-account-to-glacier",
                            "Status": "Enabled",
                            "TagFilters": [
                                {
                                    "Key": "copied_to",
                                    "Value": "open_data_account"
                                }
                            ],
                            "Transitions": [
                                {
                                    "StorageClass": "DEEP_ARCHIVE",
                                    "TransitionInDays": 1
                                }
                            ]
                        }
                    ]
                },
                "LoggingConfiguration": {
                    "DestinationBucketName": {
                        "Ref": "RestrictedFilesLogsBucketE4C14D48"
                    }
                },
                "VersioningConfiguration": {
                    "Status": "Enabled"
                }
            },
            "Type": "AWS::S3::Bucket",
            "UpdateReplacePolicy": "Retain"
        },
        "RestrictedFilesLogsBucketE4C14D48": {
            "DeletionPolicy": "Retain",
            "Properties": {
                "AccessControl": "LogDeliveryWrite",
                "BucketName": "igvf-restricted-files-logs"
            },
            "Type": "AWS::S3::Bucket",
            "UpdateReplacePolicy": "Retain"
        }
    },
    "Rules": {
        "CheckBootstrapVersion": {
            "Assertions": [
                {
                    "Assert": {
                        "Fn::Not": [
                            {
                                "Fn::Contains": [
                                    [
                                        "1",
                                        "2",
                                        "3",
                                        "4",
                                        "5"
                                    ],
                                    {
                                        "Ref": "BootstrapVersion"
                                    }
                                ]
                            }
                        ]
                    },
                    "AssertDescription": "CDK bootstrap stack version 6 required. Please run 'cdk bootstrap' with a recent version of the CDK CLI."
                }
            ]
        }
    }
}